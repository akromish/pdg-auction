AAA,EAAA,KAAA,YAAA,GAAA,CAHA;AAIA,MAAA,QAAA,EAAA,KAAA,MAJA;AAKA,MAAA,UAAA,EAAA,CACA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,MAAA,IAAA,CAAA,IAAA,6BAAA;AAAA,OADA,CALA;AAQA,MAAA,QAAA,EAAA,CACA,UAAA,CAAA;AAAA,eAAA,CAAA,IAAA,MAAA,CAAA,MAAA,4BAAA,MAAA,CAAA,MAAA,CAAA;AAAA,OADA,CARA;AAWA,MAAA,UAAA,EAAA,CACA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,MAAA,IAAA,CAAA,IAAA,gCAAA;AAAA,OADA,CAXA;AAcA,MAAA,KAAA,EAAA;AAdA,KAAA;AAgBA;AA5CA,CAAA","sourcesContent":["<template>\r\n  <v-dialog transition=\"dialog-bottom-transition\" max-width=\"600\" class=\"text-center\">\r\n    <template v-slot:activator=\"{ on, attrs }\">\r\n      <v-btn\r\n          color=\"#7b1443\"\r\n          v-bind=\"attrs\"\r\n          v-on=\"on\"\r\n          class=\"white--text\"\r\n      >Bid</v-btn>\r\n    </template>\r\n    <template v-slot:default=\"dialog\">\r\n      <v-card>\r\n        <v-toolbar\r\n            color=\"#143344\"\r\n            dark\r\n        >{{ itemName }}</v-toolbar>\r\n        <v-card-text>\r\n          <h2 class=\"mt-4\">Highest Bid: ${{ currentPrice }}({{ currentBidder }})</h2>\r\n        </v-card-text>\r\n        <v-card-actions>\r\n          <v-form ref=\"form\">\r\n            <v-text-field\r\n                label=\"name\"\r\n                v-model=\"name\"\r\n                :rules=\"inputRules\"\r\n                required\r\n            ></v-text-field>\r\n            <v-text-field\r\n                type=\"number\"\r\n                label=\"price (min $1 increment)\"\r\n                v-model.number=\"bidPrice\"\r\n                :rules=\"bidRules\"\r\n                required\r\n            ></v-text-field>\r\n            <v-text-field\r\n                label=\"phone number\"\r\n                v-model=\"phoneNumber\"\r\n                :rules=\"phoneRules\"\r\n                required\r\n            ></v-text-field>\r\n          </v-form>\r\n        </v-card-actions>\r\n        <v-card-actions>\r\n          <v-col>\r\n            <v-btn\r\n                @click=\"dialog.value = false\"\r\n            >Close</v-btn>\r\n          </v-col>\r\n          <v-col class=\"text-right\">\r\n            <v-btn\r\n                @click=\"bid(); dialog.value = false\"\r\n                color=\"#7b1443\"\r\n            ><div class=\"white--text\">Submit Bid</div></v-btn>\r\n          </v-col>\r\n        </v-card-actions>\r\n      </v-card>\r\n    </template>\r\n  </v-dialog>\r\n</template>\r\n\r\n<script>\r\nimport { db } from '@/db';\r\n\r\nexport default {\r\n  name: \"PopUp\",\r\n  methods: {\r\n    bid () {\r\n      if(this.$refs.form.validate()) {\r\n        db.collection(\"bids\").add({\r\n          name: this.name,\r\n          bidPrice: this.bidPrice,\r\n          phoneNumber: this.phoneNumber,\r\n          itemName: this.itemName\r\n        }).then( () => {\r\n          db.collection(\"items\").doc(this.itemName).update({\r\n            currentPrice: this.bidPrice,\r\n            currentBidder: this.name,\r\n            phoneNumber: this.phoneNumber,\r\n          }).then( () => {\r\n            this.$emit('changeStuff', 'hmmm');\r\n          })\r\n        })\r\n      }\r\n    },\r\n  },\r\n  props: {\r\n    itemName: String,\r\n    currentPrice: Number,\r\n    currentBidder: String,\r\n  },\r\n  data() {\r\n    return {\r\n      name: '',\r\n      phoneNumber: '',\r\n      bidMin: this.currentPrice + 1,\r\n      bidPrice: this.bidMin,\r\n      inputRules: [\r\n        v => v.length >=3 || 'please enter your full name',\r\n      ],\r\n      bidRules: [\r\n        v => v >= this.bidMin || `minimum bid: $${this.bidMin}`,\r\n      ],\r\n      phoneRules: [\r\n        v => v.length >=9 || 'please enter your phone number',\r\n      ],\r\n      valid: true,\r\n    }\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.v-toolbar {\r\n  font-size: 1.5rem;\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}