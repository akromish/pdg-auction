,MAAA,EAAA,KAAA,YAAA,GAAA,CAHA;AAIA,MAAA,QAAA,EAAA,KAAA,MAJA;AAKA,MAAA,UAAA,EAAA,CACA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,MAAA,IAAA,CAAA,IAAA,6BAAA;AAAA,OADA,CALA;AAQA,MAAA,QAAA,EAAA,CACA,UAAA,CAAA;AAAA,eAAA,CAAA,IAAA,MAAA,CAAA,MAAA,4BAAA,MAAA,CAAA,MAAA,CAAA;AAAA,OADA,CARA;AAWA,MAAA,UAAA,EAAA,CACA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,MAAA,IAAA,CAAA,IAAA,gCAAA;AAAA,OADA,CAXA;AAcA,MAAA,KAAA,EAAA;AAdA,KAAA;AAgBA;AA7CA,CAAA","sourcesContent":["<template>\n  <v-dialog transition=\"dialog-bottom-transition\" max-width=\"600\" class=\"text-center\">\n    <template v-slot:activator=\"{ on, attrs }\">\n      <v-btn\n          color=\"#7b1443\"\n          v-bind=\"attrs\"\n          v-on=\"on\"\n          class=\"white--text\"\n      >Bid</v-btn>\n    </template>\n    <template v-slot:default=\"dialog\">\n      <v-card>\n        <v-toolbar\n            color=\"#143344\"\n            dark\n        >{{ itemName }}</v-toolbar>\n        <v-card-text>\n          <h2 class=\"mt-4\">Highest Bid: ${{ currentPrice }}({{ currentBidder }})</h2>\n        </v-card-text>\n        <v-card-actions>\n          <v-form ref=\"form\">\n            <v-text-field\n                label=\"name\"\n                v-model=\"name\"\n                :rules=\"inputRules\"\n                required\n            ></v-text-field>\n            <v-text-field\n                type=\"number\"\n                label=\"price (min $1 increment)\"\n                v-model.number=\"bidPrice\"\n                :rules=\"bidRules\"\n                required\n            ></v-text-field>\n            <v-text-field\n                label=\"phone number\"\n                v-model=\"phoneNumber\"\n                :rules=\"phoneRules\"\n                required\n            ></v-text-field>\n          </v-form>\n        </v-card-actions>\n        <v-card-actions>\n          <v-col>\n            <v-btn\n                @click=\"dialog.value = false\"\n            >Close</v-btn>\n          </v-col>\n          <v-col class=\"text-right\">\n            <v-btn\n                @click=\"bid(); dialog.value = false\"\n                color=\"#7b1443\"\n            ><div class=\"white--text\">Submit Bid</div></v-btn>\n          </v-col>\n        </v-card-actions>\n      </v-card>\n    </template>\n  </v-dialog>\n</template>\n\n<script>\nimport { db } from '@/db';\n\nexport default {\n  name: \"PopUp\",\n  methods: {\n    bid () {\n      if(this.$refs.form.validate()) {\n        console.log(\"what the fleebs\");\n        db.collection(\"bids\").add({\n          name: this.name,\n          bidPrice: this.bidPrice,\n          phoneNumber: this.phoneNumber,\n          itemName: this.itemName\n        }).then( () => {\n          db.collection(\"items\").doc(this.itemName).update({\n            currentPrice: this.bidPrice,\n            currentBidder: this.name,\n            phoneNumber: this.phoneNumber,\n          }).then( () => {\n            this.$emit('changeStuff', 'hmmm');\n          })\n        })\n      }\n    },\n  },\n  props: {\n    itemName: String,\n    currentPrice: Number,\n    currentBidder: String,\n  },\n  data() {\n    return {\n      name: '',\n      phoneNumber: '',\n      bidMin: this.currentPrice + 1,\n      bidPrice: this.bidMin,\n      inputRules: [\n        v => v.length >=3 || 'please enter your full name',\n      ],\n      bidRules: [\n        v => v >= this.bidMin || `minimum bid: $${this.bidMin}`,\n      ],\n      phoneRules: [\n        v => v.length >=9 || 'please enter your phone number',\n      ],\n      valid: true,\n    }\n  },\n}\n</script>\n\n<style scoped>\n.v-toolbar {\n  font-size: 1.5rem;\n}\n</style>"],"sourceRoot":"src/components"}]}