AA,CAAA;AAAA,UAAA,IAAA,EAAA;AAAA,SAAA;AACA;AACA,KAtBA;AAuBA,IAAA,gBAvBA,8BAuBA;AACA,WAAA,IAAA,GAAA,KAAA,KAAA,CAAA,IAAA,CAAA,KAAA,CAAA,CAAA,CAAA;AACA;AAzBA,GAHA;AA8BA,EAAA,IA9BA,kBA8BA;AACA,WAAA;AACA,MAAA,IAAA,EAAA,EADA;AAEA,MAAA,WAAA,EAAA,EAFA;AAGA,MAAA,YAAA,EAAA,CAHA;AAIA,MAAA,aAAA,EAAA,cAJA;AAKA,MAAA,WAAA,EAAA,CALA;AAMA,MAAA,IAAA,EAAA,EANA;AAOA,MAAA,QAAA,EAAA,uBAPA;AAQA,MAAA,UAAA,EAAA,CACA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,MAAA,IAAA,CAAA,IAAA,kCAAA;AAAA,OADA;AARA,KAAA;AAYA;AA3CA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"text-center mx-2 my-4\">\r\n      <h1>Adding Item</h1>\r\n    </div>\r\n    <div class=\"px-2 my-3 mx-6\">\r\n      <v-form ref=\"form\">\r\n        <v-text-field\r\n          label=\"name\"\r\n          v-model=\"name\"\r\n          :rules=\"inputRules\"\r\n          required\r\n        ></v-text-field>\r\n        <v-textarea\r\n            label=\"description\"\r\n            v-model=\"description\"\r\n            :rules=\"inputRules\"\r\n            required\r\n        ></v-textarea>\r\n        <v-text-field\r\n            type=\"number\"\r\n            label=\"starting price\"\r\n            v-model.number=\"currentPrice\"\r\n            required\r\n        ></v-text-field>\r\n        <label>\r\n          <input type=\"file\" id=\"file\" ref=\"file\" v-on:change=\"handleFileUpload()\"/>\r\n        </label>\r\n        <div class=\"text-center mt-12 mb-4\">\r\n          <v-btn @click=\"pressed\">Add Item</v-btn>\r\n        </div>\r\n      </v-form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { db } from '@/db';\r\nimport firebase from \"firebase/app\";\r\nimport 'firebase/storage'\r\n\r\nexport default {\r\n  name: \"AddItem\",\r\n  title: \"Add Item\",\r\n  methods: {\r\n    pressed(){\r\n      if(this.$refs.form.validate()) {\r\n        const imgPath = `images/${this.file.name}`;\r\n        const storageRef = firebase.storage().ref();\r\n        storageRef.child(imgPath).put(this.file).then(() => {\r\n          const starsRef = storageRef.child(imgPath);\r\n          starsRef.getDownloadURL()\r\n              .then((url) => {\r\n                this.imageUrl = url;\r\n                db.collection(\"items\").doc(this.name).set({\r\n                  name: this.name,\r\n                  description: this.description,\r\n                  currentPrice: this.currentPrice,\r\n                  currentBidder: this.currentBidder,\r\n                  phoneNumber: this.phoneNumber,\r\n                  imageUrl: this.imageUrl,\r\n                })\r\n              })\r\n        })\r\n        this.$router.replace({name: 'Admin'});\r\n      }\r\n    },\r\n    handleFileUpload(){\r\n      this.file = this.$refs.file.files[0];\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      name: '',\r\n      description: '',\r\n      currentPrice: 0,\r\n      currentBidder: 'No Bids Yet!',\r\n      phoneNumber: 0,\r\n      file: '',\r\n      imageUrl: '@/assets/pdg_logo.jpg',\r\n      inputRules: [\r\n          v => v.length >=2 || 'please enter a name for the item'\r\n      ]\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":"src/views"}]}