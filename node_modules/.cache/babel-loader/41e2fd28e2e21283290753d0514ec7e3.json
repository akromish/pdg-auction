EAAA,MAAA,CAAA;AACA,YAAA,YAAA,EAAA,KAAA,CAAA,QADA;AAEA,YAAA,aAAA,EAAA,KAAA,CAAA,IAFA;AAGA,YAAA,WAAA,EAAA,KAAA,CAAA,QAHA;AAIA,YAAA,WAAA,EAAA,KAAA,CAAA,IAJA;AAKA,YAAA,IAAA,EAAA,KAAA,CAAA;AALA,WAAA;AAOA,SA1BA,EA2BA,IA3BA,CA2BA,YAAA;AACA,UAAA,KAAA,CAAA,KAAA,CAAA,aAAA,EAAA,MAAA;AACA,SA7BA;AA8BA;AACA;AAnCA,GAFA;AAuCA,EAAA,KAAA,EAAA;AACA,IAAA,QAAA,EAAA,MADA;AAEA,IAAA,YAAA,EAAA,MAFA;AAGA,IAAA,aAAA,EAAA,MAHA;AAIA,IAAA,WAAA,EAAA,MAJA;AAKA,IAAA,WAAA,EAAA;AALA,GAvCA;AA8CA,EAAA,IA9CA,kBA8CA;AAAA;;AACA,WAAA;AACA,MAAA,IAAA,EAAA,KAAA,QADA;AAEA,MAAA,QAAA,EAAA,KAAA,WAFA;AAGA,MAAA,IAAA,EAAA,KAAA,WAHA;AAIA,MAAA,QAAA,EAAA,KAAA,YAJA;AAKA,MAAA,OAAA,EAAA,KAAA,aALA;AAMA,MAAA,UAAA,EAAA,CACA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,MAAA,IAAA,CAAA,IAAA,6BAAA;AAAA,OADA,CANA;AASA,MAAA,QAAA,EAAA,CACA,UAAA,CAAA;AAAA,eAAA,CAAA,IAAA,MAAA,CAAA,MAAA,4BAAA,MAAA,CAAA,MAAA,CAAA;AAAA,OADA,CATA;AAYA,MAAA,UAAA,EAAA,CACA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,MAAA,IAAA,CAAA,IAAA,gCAAA;AAAA,OADA,CAZA;AAeA,MAAA,KAAA,EAAA;AAfA,KAAA;AAiBA;AAhEA,CAAA","sourcesContent":["<template>\r\n  <v-dialog transition=\"dialog-bottom-transition\" max-width=\"600\">\r\n    <template v-slot:activator=\"{ on, attrs }\">\r\n      <v-btn\r\n          color=\"#7b1443\"\r\n          v-bind=\"attrs\"\r\n          v-on=\"on\"\r\n          class=\"white--text\"\r\n      >Edit</v-btn>\r\n    </template>\r\n    <template v-slot:default=\"dialog\">\r\n      <v-card>\r\n        <v-toolbar\r\n            color=\"#143344\"\r\n            dark\r\n        >{{ itemName }}</v-toolbar>\r\n        <v-card-actions>\r\n          <v-form ref=\"form\">\r\n            <v-text-field\r\n                label=\"name\"\r\n                v-model=\"name\"\r\n                :rules=\"inputRules\"\r\n                required\r\n            ></v-text-field>\r\n            <v-textarea\r\n                label=\"description\"\r\n                v-model=\"desc\"\r\n                :rules=\"inputRules\"\r\n                required\r\n            ></v-textarea>\r\n            <v-text-field\r\n                type=\"number\"\r\n                label=\"price\"\r\n                v-model.number=\"bidPrice\"\r\n                required\r\n            ></v-text-field>\r\n            <v-text-field\r\n                label=\"bidder\"\r\n                v-model=\"bidName\"\r\n                required\r\n            ></v-text-field>\r\n            <v-text-field\r\n                label=\"phone number\"\r\n                v-model=\"phoneNum\"\r\n                required\r\n            ></v-text-field>\r\n          </v-form>\r\n        </v-card-actions>\r\n        <v-card-actions>\r\n          <v-col>\r\n            <v-btn\r\n                @click=\"dialog.value = false\"\r\n            >Cancel</v-btn>\r\n          </v-col>\r\n          <v-col class=\"text-right\">\r\n            <v-btn\r\n                @click=\"modify(); dialog.value = false\"\r\n                color=\"#7b1443\"\r\n            ><div class=\"white--text\">Confirm Edit</div>\r\n            </v-btn>\r\n          </v-col>\r\n        </v-card-actions>\r\n      </v-card>\r\n    </template>\r\n  </v-dialog>\r\n</template>\r\n\r\n<script>\r\nimport { db } from '@/db';\r\n\r\nexport default {\r\n  name: \"PopUp\",\r\n  methods: {\r\n    modify () {\r\n      if(this.$refs.form.validate()) {\r\n        let docId = \"\";\r\n        db.collection(\"bids\")\r\n        .where(\"name\", \"==\", this.bidName)\r\n        .where(\"bidPrice\", \"==\", this.bidPrice)\r\n        .where(\"itemName\", \"==\", this.name)\r\n        .get()\r\n        .then((querySnapshot) => {\r\n          querySnapshot.forEach((doc) => {\r\n            docId = doc.id\r\n          });\r\n        })\r\n        .then( () => {\r\n          db.collection(\"bids\").doc(docId).update({\r\n            name: this.bidName,\r\n            bidPrice: this.bidPrice,\r\n            phoneNumber: this.phoneNum,\r\n            itemName: this.name\r\n          })\r\n        })\r\n        .then( () => {\r\n          db.collection(\"items\").doc(this.itemName).update({\r\n            currentPrice: this.bidPrice,\r\n            currentBidder: this.name,\r\n            phoneNumber: this.phoneNum,\r\n            description: this.desc,\r\n            name: this.name,\r\n          })\r\n        })\r\n        .then( () => {\r\n          this.$emit('changeStuff', 'hmmm');\r\n        })\r\n      }\r\n    },\r\n  },\r\n  props: {\r\n    itemName: String,\r\n    currentPrice: Number,\r\n    currentBidder: String,\r\n    description: String,\r\n    phoneNumber: String,\r\n  },\r\n  data() {\r\n    return {\r\n      name: this.itemName,\r\n      phoneNum: this.phoneNumber,\r\n      desc: this.description,\r\n      bidPrice: this.currentPrice,\r\n      bidName: this.currentBidder,\r\n      inputRules: [\r\n        v => v.length >=3 || 'please enter your full name',\r\n      ],\r\n      bidRules: [\r\n        v => v >= this.bidMin || `minimum bid: $${this.bidMin}`,\r\n      ],\r\n      phoneRules: [\r\n        v => v.length >=9 || 'please enter your phone number',\r\n      ],\r\n      valid: true,\r\n    }\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.v-toolbar {\r\n  font-size: 1.5rem;\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}