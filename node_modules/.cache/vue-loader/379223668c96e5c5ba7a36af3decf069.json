rement\"\n            v-model=\"increment\"\n        ></v-text-field>\n        <v-btn @click=\"pressed\">Add Item</v-btn>\n      </v-form>\n    </v-app>\n  </div>\n</template>\n\n<script>\nimport { db } from '@/db';\n\nexport default {\n  name: \"AddItem\",\n  methods: {\n    pressed(){\n      db.collection(\"items\").add({\n        name: this.name,\n        description: this.description,\n        increment: this.increment,\n        currentPrice: this.startingPrice,\n        currentBidder: \"\",\n        bid: false\n      })\n      this.$router.replace({name: 'Admin'});\n\n    }\n  },\n  data() {\n    return {\n      name: '',\n      description: '',\n      startingPrice: 0,\n      increment: 0,\n      currentPrice: 0,\n    }\n  }\n}\n</script>\n\n<style scoped>\n/*.adding {*/\n/*  margin: auto;*/\n/*  margin-top: 30px;*/\n/*  width: 50%;*/\n/*}*/\n</style>"]}]}